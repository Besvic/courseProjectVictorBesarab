<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="process.controller.EnterInAccount">
   <children>
      <ImageView fitHeight="343.0" fitWidth="600.0" layoutY="57.0" pickOnBounds="true">
         <image>
            <Image url="@../../image/start/firstImage.jpg" />
         </image>
      </ImageView>
      <Label layoutX="-1.0" prefHeight="57.0" prefWidth="594.0" text="    Konsult.ru">
         <font>
            <Font name="Castellar" size="53.0" />
         </font>
      </Label>
      <AnchorPane layoutX="378.0" layoutY="57.0" prefHeight="343.0" prefWidth="222.0" style="-fx-background-color: Moccasin;">
         <children>
            <Button fx:id="buttonSignIn" layoutX="84.0" layoutY="226.0" mnemonicParsing="false" onAction="#buttonEnterAction" prefHeight="32.0" prefWidth="77.0" style="-fx-background-color: silver; -fx-background-radius: 15; -fx-border-color: black; -fx-border-radius: 15;" text="Вход" textAlignment="CENTER">
               <font>
                  <Font name="Calibri Italic" size="21.0" />
               </font>
            </Button>
            <TextField fx:id="textFieldLogin" layoutX="40.0" layoutY="94.0" prefHeight="32.0" prefWidth="164.0" promptText="Логин" style="-fx-background-radius: 8; -fx-border-color: black; -fx-border-radius: 8;">
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
            <ChoiceBox fx:id="choiceLaw" layoutX="29.0" layoutY="14.0" prefHeight="32.0" prefWidth="164.0">
               <effect>
                  <Blend />
               </effect>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </ChoiceBox>
            <Button fx:id="buttonRegister" layoutX="46.0" layoutY="281.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="153.0" style="-fx-background-color: silver; -fx-background-radius: 15; -fx-border-color: black; -fx-border-radius: 15;" text="Регистрация" textAlignment="CENTER">
               <font>
                  <Font name="Calibri Italic" size="21.0" />
               </font>
            </Button>
            <PasswordField fx:id="textFieldPassword" layoutX="41.0" layoutY="172.0" prefHeight="32.0" prefWidth="164.0" promptText="Пароль" style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-border-color: black;" />
            <Label layoutX="50.0" layoutY="69.0" text="Логин:" />
            <Label layoutX="46.0" layoutY="147.0" text="Пароль:" />
         </children></AnchorPane>
   </children>
</AnchorPane>
